{
  "name": "graphql-expressjs-boilerplate",
  "version": "1.0.0",
  "description": "Starter for a flexible and scalable GraphQL server for Nodejs with Expressjs",
  "author": "Leonardo B. Castro",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js --inspect",
    "start": "NODE_ENV=production babel-node src/index.js",
    "orm": "npx sequelize",
    "lint": "eslint .",
    "test": "NODE_ENV=test jest --colors --testPathIgnorePatterns tests/test.js",
    "pretest": "node scripts/remove-db-test.js && NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
    "posttest": "NODE_ENV=test sequelize db:seed:undo:all && NODE_ENV=test sequelize db:migrate:undo:all",
    "envs:generate": "node scripts/create-envs.js",
    "jobs:up": "babel-node jobs/index.js&"
  },
  "jest": {
    "verbose": true,
    "bail": 1,
    "browser": false,
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!**/node_modules/**"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@elastic/elasticsearch": "^7.9.1",
    "aws-sdk": "^2.771.0",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.18.2",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.7.0",
    "graphql-tools": "^4.0.8",
    "graphql-upload": "^8.1.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "mongoose": "^5.10.9",
    "node-cron": "^2.0.3",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "redis": "^2.8.0",
    "sequelize": "^5.22.3",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.11.5",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^24.9.0",
    "eslint": "^4.19.1",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "redis-mock": "^0.46.0",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.2.0",
    "supertest": "^4.0.2"
  }
}
